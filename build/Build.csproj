        <PackageReference Include="JsonSchema.Net" Version="5.4.1" />
        <PackageReference Include="Cake.Frosting" Version="4.0.0" />
        <PackageReference Include="Cake.NuGet" Version="4.0.0" />
        <PackageReference Include="NuGet.Frameworks" Version="6.9.1" />
        <PackageReference Include="NuGet.Protocol" Version="6.8.0" />
        <PackageReference Include="Magick.NET-Q8-AnyCPU" Version="13.6.0" />
        <PackageReference Include="Samboy063.Tomlet" Version="5.3.0" />
        <PackageReference Include="Semver" Version="2.3.0" />
        <PackageReference Include="Serilog" Version="3.1.1" />
        <PackageReference Include="Crayon" Version="2.0.69" />
        <PackageReference Include="LibGit2Sharp" Version="0.29.0" />
    </ItemGroup>
</Project>
